[flake8]
ignore = E203, E266, E501, W503, F403, F401, F541
exclude = 
    .git,
    .gitignore,
    .pre-commit-config.yaml,
    .travis.yml,
    pyproject.toml,
    cloud_sql_proxy,
    src/__pycache__,
    tests/__pycache__,
    .pytest_cache,
    .vscode,
    venv
max-line-length = 79
max-complexity = 18
select = B,C,E,F,W,T4,B9